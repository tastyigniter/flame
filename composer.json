{
  "name": "tastyigniter/flame",
  "description": "TastyIgniter Flame core library of TastyIgniter, add some flame to laravel",
  "homepage": "https://tastyigniter.com",
  "keywords": ["tastyigniter", "igniter", "flame", "laravel"],
  "license": "MIT",
  "authors": [
    {
      "name": "Samuel Adepoyigi",
      "email": "sam@sampoyigi.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "fideloper/proxy": "~3.3",
    "filp/whoops": "~2.0",
    "doctrine/instantiator": "~1.0",
    "guzzlehttp/guzzle": "~6.3",
    "symfony/yaml": "~3.2",
    "october/rain": "~1.0",
    "kalnoy/nestedset": "~4.3",
    "watson/validating": "~3.1",
    "natxet/CssMin": "~3.0",
    "leafo/scssphp": "~0.7",
    "kriswallsmith/assetic": "^1.4",
    "league/glide": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "swiftmailer/swiftmailer": "~6.0",
    "laravel/tinker": "~1.0",
    "laravel/dusk": "~1.0",
    "mockery/mockery": "~0.9",
    "fzaninotto/faker": "~1.4"
  },
  "autoload": {
    "files": [
      "src/Support/helpers.php"
    ],
    "psr-4": {
      "Igniter\\Flame\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev"
}
