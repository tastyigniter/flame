{
  "name": "tastyigniter/flame",
  "description": "TastyIgniter Flame core library of TastyIgniter, add some flame to laravel",
  "homepage": "https://tastyigniter.com",
  "keywords": [
    "tastyigniter",
    "igniter",
    "flame",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Samuel Adepoyigi",
      "email": "sam@sampoyigi.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-PDO": "*",
    "ext-zip": "*",
    "fideloper/proxy": "~4.0",
    "doctrine/instantiator": "~1.0",
    "october/rain": "~1.0.468",
    "guzzlehttp/guzzle": "~6.3",
    "kalnoy/nestedset": "~5.0",
    "watson/validating": "~4.0",
    "natxet/cssmin": "~3.0",
    "scssphp/scssphp": "~1.0",
    "league/glide": "~1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0",
    "illuminate/database": "^6.0",
    "illuminate/encryption": "^6.0",
    "illuminate/filesystem": "^6.0",
    "illuminate/queue": "^6.0",
    "illuminate/support": "^6.0",
    "illuminate/log": "^6.0",
    "illuminate/mail": "^6.0",
    "illuminate/events": "^6.0",
    "illuminate/pagination": "^6.0",
    "illuminate/routing": "^6.0",
    "illuminate/validation": "^6.0",
    "illuminate/view": "^6.0",
    "illuminate/cache": "^6.0",
    "mockery/mockery": "^1.0",
    "fzaninotto/faker": "^1.4"
  },
  "suggest": {
    "ext-pdo_mysql": "Required to use MySQL databases"
  },
  "replace": {
    "kriswallsmith/assetic": "*",
    "leafo/scssphp": "*"
  },
  "autoload": {
    "files": [
      "src/Support/helpers.php"
    ],
    "psr-4": {
      "Igniter\\Flame\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
